            $(ecModal).fadeIn(200);
            return false;
        });
    });
    $('.ec-modal-close').on('click',function(){
        $('.ec-modal').fadeOut( 200);
    		$('body').removeClass("is-modal-open");
        $('body,html').stop().animate({scrollTop:winScrollTop}, 100);
        return false;
    });
});
</script>



